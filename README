  /usr/bin/puid.exe
  /usr/lib/tunepimp/plugins/flac.tpp
  /usr/lib/tunepimp/plugins/mp3.tpp
  /usr/lib/tunepimp/plugins/mp4.tpp
  /usr/lib/tunepimp/plugins/mpc.tpp
  /usr/lib/tunepimp/plugins/speex.tpp
  /usr/lib/tunepimp/plugins/tta.tpp
  /usr/lib/tunepimp/plugins/vorbis.tpp
  /usr/lib/tunepimp/plugins/wav.tpp
  /usr/lib/tunepimp/plugins/wma.tpp
  /usr/lib/tunepimp/plugins/wv.tpp
  /usr/share/doc/Cygwin/libtunepimp-0.5.0.README
  /usr/share/doc/libtunepimp-0.5.0/AUTHORS
  /usr/share/doc/libtunepimp-0.5.0/COPYING
  /usr/share/doc/libtunepimp-0.5.0/ChangeLog
  /usr/share/doc/libtunepimp-0.5.0/INSTALL
  /usr/share/doc/libtunepimp-0.5.0/README
  /usr/share/doc/libtunepimp-0.5.0/TODO

Files included in the runtime library package:

  /usr/bin/cygtunepimp-5.dll

Files included in the development package:

  /usr/include/tunepimp-0.5/context.h
  /usr/include/tunepimp-0.5/decode_plugin.h
  /usr/include/tunepimp-0.5/defs.h
  /usr/include/tunepimp-0.5/fileio.h
  /usr/include/tunepimp-0.5/metadata_plugin.h
  /usr/include/tunepimp-0.5/tp_c.h
  /usr/include/tunepimp-0.5/track.h
  /usr/include/tunepimp-0.5/tunepimp.h
